{
  "minimum-stability": "stable",
  "prefer-stable": true,
  "config": {
    "vendor-dir": "vendor",
    "preferred-install": {
      "*": "dist"
    },
    "platform-check": false,
    "autoloader-suffix": "PPChecklists"
  },
  "require": {
    "php": ">=7.2.5",
    "publishpress/psr-container": "^2.0",
    "publishpress/pimple-pimple": "^3.5",
    "publishpress/vendor-locator-checklists": "^1.0",
    "publishpress/wordpress-version-notices": "^2.1",
    "publishpress/wordpress-reviews": "^1.1",
    "publishpress/instance-protection": "^1.0"
  }
}